    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.1",
    "serve": "^14.1.2",
    "typescript": "4.9.4",
    "yarn-run-all": "^3.1.1"
