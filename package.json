    "@types/react": "18.0.14",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "serve": "^13.0.2",
    "typescript": "4.7.4",
    "yarn-run-all": "^3.1.1"
  }
