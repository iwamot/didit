    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "prettier": "^2.5.1",
    "serve": "^13.0.2",
    "typescript": "4.5.5",
    "yarn-run-all": "^3.1.1"
  }
